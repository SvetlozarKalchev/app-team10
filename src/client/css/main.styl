// out: main.css, sourcemap: false, compress: true
@import "normalize"

// Color variables go here
dark_green = #eee

html, body
  box-sizing border-box
  height 100%
  width 100%
  overflow-x hidden

body
  padding 10px

// Start screen styles
.start_screen_content
  width 100%
  height 100%
  text-align center

.start_screen_content button
  width 200px
  height 80px
  margin-top 50px

.start_screen_content progress
  display block
  width 200px
  height 20px
  margin 0 auto
  margin-top 50px
  background-color: dark_green;
  border-radius: 2px;
  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.25)
